void fragment() {
	vec2 uv = SCREEN_UV;
	vec4 color = texture(SCREEN_TEXTURE, uv);
	float avg = (color.r + color.g + color.b) / 3.0;
	color = mix(vec4(avg, avg, avg, color.a), vec4(0.0, 0.0, 1.0, color.a), 0.4);
	color.b += min(step((sin((uv.x / SCREEN_PIXEL_SIZE.x) * 0.5) / 2.0) + 0.5, 0.1) + step((sin((uv.y / SCREEN_PIXEL_SIZE.y) * 0.5) / 2.0) + 0.5, 0.1), 1.0) * 0.1;
	COLOR = color;
}"

